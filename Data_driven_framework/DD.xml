<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Logincases">
	
 <test name="Logincases">
     <classes>
       <class name="Dataprovider.Logincases"/>
       </classes>
 </test><!-- Test -->
	
 
 
  <test name="Invalid_credentials">
	 <parameter name="username" value="prabhu12343423"/> 
	 <parameter name="password" value="12345234"/>
     <classes>
       <class name="Logincases.Invalid_credentials"/>
       </classes>
 </test><!-- Test -->
 
  <test name="Valid_username">
	 <parameter name="username" value="prabhu1234"/>
	 <parameter name="password" value="1234gsg5"/>
     <classes>
       <class name="Logincases.Valid_username"/>
       </classes>
 </test><!-- Test -->
 
  <test name="Valid_Password">
	 <parameter name="username" value="prabhu"/>
	 <parameter name="password" value="12345"/>
     <classes>
       <class name="Logincases.Valid_Password"/>
       </classes>
 </test><!-- Test -->
 
 <test name="Valid_credentials">
	 <parameter name="username" value="prabhu123"/>
	 <parameter name="password" value="12345"/>
     <classes>
       <class name="Logincases.Valid_credentials"/>
     </classes>
 </test><!-- Test --> 
 
 
</suite><!-- Test -->